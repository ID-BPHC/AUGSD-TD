		script.
			let stage1 = ["https://code.getmdl.io/1.3.0/material.min.js","/scripts/jquery-3.2.1.slim.js","/scripts/outdatedbrowser.min.js"];
			let stage2 = ["/stylesheets/outdatedbrowser.min.css", "https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&lang=en", "https://fonts.googleapis.com/icon?family=Material+Icons", "https://code.getmdl.io/1.3.0/material.min.css", "/stylesheets/indexstyle.css"]
			let asyncLoad = new CSSLoader();
			stage2.forEach((item) => {asyncLoad.add(item)});
			asyncLoad.load();
			asyncLoad = new ScriptLoader();
			stage1.forEach((item) => {asyncLoad.add(item)});
			asyncLoad.load(addLoadEvent(function(){
				outdatedBrowser({
					bgColor: '#f25648',
